{
  "name": "language-elvish",
  "displayName": "Elvish Syntax Highlighting",
  "description": "Elvish Shell language support",
  "version": "0.0.1",
  "publisher": "champii",
  "engines": {
    "vscode": "^1.23.0"
  },
  "icon": "images/logo.png",
  "galleryBanner": {
		"color": "#293845",
		"theme": "dark"
	},
  "repository": {
		"type": "git",
		"url": "https://github.com/Champii/vscode-elvish.git"
	},
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "elvish",
        "aliases": [
          "Elvish",
          "elvish"
        ],
        "extensions": [
          ".elv"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "elvish",
        "scopeName": "source.elvish",
        "path": "./syntaxes/elvish.tmLanguage.json"
      }
    ]
  }
}
